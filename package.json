  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www --ignore ./public/ --ignore ./app/",
    "test": "NODE_PATH='./:./lib' mocha --compilers js:babel-core/register --require ./test/setup.js --require ignore-styles \"test/**/*@(.js)\"",
    "test:watch": "npm run test -- --reporter min --watch",
    "build": "webpack --progress --colors --display-error-details --config webpack/webpack.config.js",
    "buildProd": "NODE_ENV=production webpack --progress --colors --display-error-details --config webpack/webpack.production.config.js",
    "updateEmailTemplates": "node ./app/email/update.js",
    "updateEmailTemplatesDev": "NODE_ENV=development node ./app/email/update.js",
    "watchEmailTemplates": "nodemon --watch ./app/email/ --watch ./app/sass --watch ./app/email/render.js --ext hbs,scss,js ./app/email/render.js"
  },
