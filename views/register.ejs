<section id="signup">
	<div>
		<% if (!token){ %>
			<h2>Sign up for<br>Fresco News</h2>
			<span>Your account will be the primary account for your newsroom.<br><br>Once you're logged in, you can invite everyone else by email, or automatically allow signups from your company email.</span>
		<% }else{ %>
			<h2>Join <%= outlet_title %> on<br>Fresco News</h2>
		<% } %>
		<span id="register-error"></span>
		<form>
			<input type="text" id="fn" placeholder="First name" spellcheck="false">
			<input type="text" id="ln" placeholder="Last name" spellcheck="false">
			<%
				if (!token){
			%>
			<input type="text" id="outlet" placeholder="Outlet Title (optional)" spellcheck="false">
			<input type="text" id="em" placeholder="Email address" spellcheck="false">
			<%
				}else{
			%>
			<input type="hidden" id="em" value="<%= email %>">
			<input type="hidden" id="outlet" value="">
			<%
				}
			%>
			<input type="password" id="pw" placeholder="Password" spellcheck="false">
			<input type="password" id="c_pw" placeholder="Confirm password" spellcheck="false">
			<input id="register-submit" type="submit" value="Submit">
		</form>
	</div>
</section>
