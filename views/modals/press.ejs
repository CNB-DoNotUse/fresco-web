<div class="modal modal_window press xs <%- active %>" id="_press">
	<div class="header">
		<h2>Fresco in the News</h2>
		
		<span data-event="landing" class="mdi mdi-close modal-action"></span>
	</div>
	
	<div class="body">
		<ul class="articles">
			
			<% for(var i=0; i < articles.length; i++) { %>

				<% article = articles[i] %>

				<li>
					<div class="preview">
						<img src="<%= utils.CDN %>/index/<%- article.image %>.png" />
					</div>
					<div class="meta">
						<a target="_blank" href="<%- article.url %>">
							<h3><%- article.title %></h3>
						</a>
						
						<p class="author"><%- article.author %> &bull; <%- article.source %></p>
						
						<% if (article.quote) { %>
							<div class="description"><%- article.quote %></div>
						<% } %>

					</div>
				</li>

			<% } %>
		</ul>
	</div>

	<div class="modal-footer">
		<ul>
			<li class="pdf-kit">
				<a href="<%= utils.CDN %>/pdf/press.pdf" target="_blank">
					<span class="mdi mdi-download"></span>
					<p></p>
				</a>
			</li>
			<li class="drive-kit">
				<a href="https://drive.google.com/open?id=0B50wJrfKYm14fkhLdy02anRhM1VoTm1ZekoxZEQxTlpZSTBDWmZ2bnFNbGZ3amlYTW0yWlU" target="_blank">
					<span class="mdi mdi-google-drive"></span>
					<p></p>
				</a>
			</li>
		</ul>
	</div>
</div>