<div class="modal team <%- active %>" id="_team">
	<div class="header">
		<h2>Team</h2>
		
		<span data-event="landing" class="mdi mdi-close modal-action"></span>
	</div>
	
	<div class="body">
		<div class="banner">
			<img src="https://d1dw1p6sgigznj.cloudfront.net/index/team.jpg">
		</div>

		<div class="team-rows">
			<% for(var i=0; i <team.length; i++) { %>
				<div class="row">
					<% if(typeof(team[i]['title']) != 'undefined'){ %>
						<h3><%= team[i]['title'] %></h3>
					<% } %>	

					<ul>
						<% for(var k=0; k < team[i]['members'].length; k++) { %>

							<% member = team[i]['members'][k] %>

							<li>
								<% if(typeof(member['avatar']) != 'undefined'){ %>
									<div class="avatar">
										<img src="http://cdn.static.fresconews.com/index/team/<%= member['avatar'] %>" />
									</div>
								<% } else { %>
									<div class="avatar border"></div>
								<% } %>
								<div class="meta">
									<p class="name"><%- member['name'] %></p>
									<p class="title"><%- member['title'] %></p>
									<% if(typeof(member['handle']) != 'undefined'){ %>
										<a target="_blank" href="https://twitter.com/<%= member['handle'] %>">
											<p class="handle">@<%= member['handle'] %></p>
										</a>
									<% } %>
								</div>
							</li>
						<% } %>
					</ul>
				</div>

		    <% } %>
		</div>
	</div>
</div>