<div class="modal account <%- active %>" id="_account">
	<div id="_presentation" class="presentation">
		<iframe width="100%" height="100%" src="https://www.youtube.com/embed/I3Dhw6YCT-U" frameborder="0" allowfullscreen></iframe>
	</div>

	<div class="form-wrap login" id="_login">
		<form id="loginForm">
			<h3>Log In</h3>
			<span class="mdi mdi-help-circle help modal-action" data-event="modal" data-modal="forgot"></span>
