<script type="text/javascript" src="/javascripts/scrollspy.js"></script>
<script type="text/javascript" src="/javascripts/snackbar.min.js"></script>
<script type="text/javascript" src="/javascripts/fresco.js"></script>
<% if (token){ %><script type="text/javascript">var oref = '<%= token %>';</script><% } %>
<script type="text/javascript" src="/javascripts/handlers/join.js"></script>
<script type="text/javascript">
	$(document).ready(function(){<%- JSON.stringify(alerts) %>.forEach(function(alert){$.snackbar({content: alert, timeout: 5000, htmlAllowed: true});});});
</script>
